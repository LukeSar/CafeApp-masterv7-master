<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1C1C1C">

    <TextView
        android:id="@+id/textView"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:text="Lunch Menu"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView"
        android:layout_marginTop="12dp"
        android:text="Tap to add item (Edit order at checkout)" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView2"
        android:layout_marginLeft="19dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="17dp"
        android:text="Food"
        android:textColor="@android:color/black"
        android:textColorHighlight="@android:color/black" />

    <ImageButton
        android:id="@+id/BackButton"
        android:layout_width="70dp"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@android:color/transparent"
        app:srcCompat="?attr/actionModeCloseDrawable" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3"
        android:layout_below="@+id/PastaButton"
        android:text="Drinks"
        android:textColor="@android:color/black" />

    <Button
        android:id="@+id/GrilledChickenButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/EscalopeChickenButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/EscalopeChickenButton"
        android:layout_below="@+id/textView3"
        android:text="Grilled Chicken (2 Breasts)"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/EscalopeChickenButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/KebabButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/KebabButton"
        android:layout_below="@+id/GrilledChickenButton"
        android:text="Chicken Escalope (2 Breasts)"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/KebabButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/EscalopeChickenButton"
        android:text="Chicken Kebab (1 Skewer Large)"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/FishCakeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/KebabButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/KebabButton"
        android:layout_below="@+id/KebabButton"
        android:text="Fish Cake"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/SaladBoxButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/FishCakeButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/FishCakeButton"
        android:layout_below="@+id/FishCakeButton"
        android:text="Salad Box "
        android:textAllCaps="false" />

    <Button
        android:id="@+id/CiabattaButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/SaladBoxButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/SaladBoxButton"
        android:layout_below="@+id/SaladBoxButton"
        android:text="Add into Ciabatta Bread"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/PastaButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/CiabattaButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/CiabattaButton"
        android:layout_below="@+id/CiabattaButton"
        android:text="Pasta Box (Tomato and Beef)"
        android:textAllCaps="false" />

    <ImageButton
        android:id="@+id/InfoImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="17dp"
        android:layout_marginRight="17dp"
        android:background="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_menu_info_details" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/GrilledChickenButton"
        android:layout_marginTop="13dp"
        android:layout_toEndOf="@+id/textView"
        android:layout_toRightOf="@+id/textView"
        android:text="£3.00" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/KebabButton"
        android:layout_alignEnd="@+id/textView5"
        android:layout_alignRight="@+id/textView5"
        android:layout_marginBottom="13dp"
        android:text="£4.00" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/FishCakeButton"
        android:layout_alignEnd="@+id/textView6"
        android:layout_alignRight="@+id/textView6"
        android:layout_marginBottom="12dp"
        android:text="£2.50" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/textView7"
        android:layout_marginTop="13dp"
        android:text="£3.00" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/SaladBoxButton"
        android:layout_alignBottom="@+id/SaladBoxButton"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8"
        android:text="£3.50" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/CiabattaButton"
        android:layout_alignBottom="@+id/CiabattaButton"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8"
        android:text="£1.50" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/PastaButton"
        android:layout_alignBottom="@+id/PastaButton"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:text="£5.00" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/InfoImage"
        android:layout_alignRight="@+id/InfoImage"
        android:layout_alignTop="@+id/GrilledChickenButton"
        android:background="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_menu_info_details" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/imageButton7"
        android:layout_alignRight="@+id/imageButton7"
        android:layout_below="@+id/GrilledChickenButton"
        android:background="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_menu_info_details" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/imageButton9"
        android:layout_alignRight="@+id/imageButton9"
        android:layout_below="@+id/textView6"
        android:background="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_menu_info_details" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/imageButton10"
        android:layout_alignRight="@+id/imageButton10"
        android:layout_below="@+id/textView7"
        android:background="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_menu_info_details" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/imageButton11"
        android:layout_alignRight="@+id/imageButton11"
        android:layout_below="@+id/FishCakeButton"
        android:background="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_menu_info_details" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/imageButton12"
        android:layout_alignRight="@+id/imageButton12"
        android:layout_below="@+id/SaladBoxButton"
        android:background="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_menu_info_details" />

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/imageButton13"
        android:layout_alignRight="@+id/imageButton13"
        android:layout_below="@+id/CiabattaButton"
        android:background="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_menu_info_details" />
</RelativeLayout>